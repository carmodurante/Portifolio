<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout">
    <Page id="page" title="{i18n>title}">
        <content>
            <Button id="BadgedButton" class="sapUiTinyMarginBeginEnd" text="Reiniciar" press="onClickReiniciar">
            </Button>
            <VBox id="_IDGenVBox1" class="sapUiTinyMarginTop">
                <HBox id="_IDGenHBox1" justifyContent="Center" class="sapUiTinyMarginTop">
                    <Image id="casa1" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                    <Image id="casa2" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                    <Image id="casa3" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                </HBox>
                <HBox id="_IDGenHBox2" justifyContent="Center" class="sapUiTinyMarginTop">
                    <Image id="casa4" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                    <Image id="casa5" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                    <Image id="casa6" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                </HBox>

                <HBox id="_IDGenHBox3" justifyContent="Center" class="sapUiTinyMarginTop">
                    <Image id="casa7" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                    <Image id="casa8" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                    <Image id="casa9" src="../img/Branco.png" class="sapUiTinyMarginEnd" press="onClickCasa" />
                </HBox>
            </VBox>
        </content>
    </Page>
</mvc:View>
